{
    "name": "native",
    "main": "expo-router/entry",
    "version": "1.0.0",
    "scripts": {
        "clean": "git clean -xdf .expo node_modules ios android dist",
        "start": "expo start --clear",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "format": "prettier --write .",
        "typecheck": "tsc --noEmit",
        "gen:convert": "swagger2openapi ../docs/swagger.yaml -o ./types/schema.yaml",
        "gen:client": "openapi-typescript ./types/schema.yaml -o ./types/schema.d.ts",
        "gen": "bun gen:convert && bun gen:client",
        "lint": "eslint . --fix"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.69.0",
        "expo": "^52.0.16",
        "expo-constants": "~17.0.3",
        "expo-linking": "~7.0.3",
        "expo-router": "~4.0.11",
        "expo-splash-screen": "~0.29.13",
        "expo-status-bar": "~2.0.0",
        "nativewind": "^4.1.23",
        "openapi-fetch": "^0.13.5",
        "openapi-react-query": "^0.3.1",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-native": "0.76.6",
        "react-native-reanimated": "~3.16.1",
        "react-native-safe-area-context": "4.12.0",
        "react-native-screens": "~4.4.0",
        "react-native-web": "~0.19.6",
        "tailwindcss": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@eslint/js": "^9.23.0",
        "@types/react": "~18.3.12",
        "eslint": "^8.57.0",
        "openapi-typescript": "^7.6.1",
        "prettier": "3.5.3",
        "swagger2openapi": "^7.0.8",
        "typescript": "~5.3.3",
        "typescript-eslint": "^8.28.0"
    }
}
